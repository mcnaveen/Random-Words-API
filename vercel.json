{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/swagger-ui-bundle.js",
      "dest": "/public/swagger-ui-bundle.js"
    },
    {
      "src": "/swagger-ui-standalone-preset.js",
      "dest": "/public/swagger-ui-standalone-preset.js"
    },
    {
      "src": "/swagger-ui.min.css",
      "dest": "/public/swagger-ui.min.css"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}